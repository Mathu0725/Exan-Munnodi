    "test:e2e:ui": "playwright test --ui",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "node prisma/seed.js",
    "db:up": "docker compose up -d postgres pgbouncer",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f postgres pgbouncer",
    "prisma:pg:generate": "prisma generate --schema=prisma/schema.postgres.prisma",
    "prisma:pg:push": "prisma db push --schema=prisma/schema.postgres.prisma",
    "prisma:pg:migrate": "prisma migrate dev --schema=prisma/schema.postgres.prisma",
    "prisma:pg:studio": "prisma studio --schema=prisma/schema.postgres.prisma",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
