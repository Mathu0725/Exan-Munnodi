  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "jest --coverage --coverageReporters=html --coverageReporters=text --coverageReporters=lcov --coverageReporters=json-summary",
    "test:badges": "npm run test:coverage:report && node scripts/generate-coverage-badge.js",
